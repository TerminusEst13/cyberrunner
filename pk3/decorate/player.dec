      Pain2:
        PLAY G 4 BRIGHT A_Pain
        Goto Spawn

      XDeath:
      Death:
        TNT1 A 0
        PLAY H 10 BRIGHT ACS_ExecuteAlways(401)
        PLAY I 10 BRIGHT A_PlayerScream
        PLAY J 10 BRIGHT A_NoBlocking
        PLAY KLM 10 BRIGHT
        PLAY N -1 BRIGHT
        Stop
    }
}

