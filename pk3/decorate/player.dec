actor CyberRunner : PlayerPawn
{
    Speed 1
    Radius 16
    Height 56
    Mass 100
    PainChance 255
    Health 200
    Player.MaxHealth 200
    Gravity 0.8
    Player.DisplayName "CyberRunner"
    Player.CrouchSprite "PLYC"
    Player.StartItem "Plasma Gun", 1
