      Spawn:
        PLAY A 1 BRIGHT A_JumpIfInventory("DashCooldown",29,"CyberMove")
        loop
        
        // MOVEMENT STATES
      See:
        PLAY A 1 BRIGHT A_JumpIfInventory("JumpCooldown",29,"CyberMove")
        PLAY A 1 BRIGHT A_JumpIfInventory("DashCooldown",29,"CyberMove")
        PLAY A 0 A_JumpIfInventory("CyberBoostSpeed",1,"Dash1")
